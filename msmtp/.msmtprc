# Set default values for all following accounts.
defaults
auth           on
tls            on
logfile        ~/.msmtp.log


# account will8clark
# host smtp.gmail.com
# port 587
# tls_trust_file /private/etc/ssl/cert.pem
# protocol smtp
# from will8clark@gmail.com
# user will8clark@gmail.com

account 256k1
host smtp.mailbox.org
port 587
protocol smtp
from will@256k1.dev
user will@256k1.dev
passwordeval gpg --no-tty --quiet --for-your-eyes-only --decrypt /home/will/.mutt/msmtp-256k1-new.gpg
timeout 20

account default : 256k1
